{
  "name": "photobuddy",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:stuf/photobuddy.git",
    "type": "git"
  },
  "scripts": {
    "postinstall": "nfuse",
    "dev": "npm run dev:start && npm run fuse:start:local",
    "dev:watch": "babel . --watch --out-dir . --only \"./**/*.es6\" --ignore \"./node_modules/**\"",
    "dev:start": "babel . --out-dir . --only \"./**/*.es6\" --ignore \"./node_modules/**\"",
    "fuse:start:local": "concurrently -k \"npm run dev:watch\" \"fuse preview\""
  },
  "author": "Stefan Rimaila <stefan@rimaila.fi>",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015-fuse": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
    "concurrently": "^3.1.0",
    "nfuse": "^2.2.3"
  }
}
